<body>
<div id="home"></div>
<nav class="navbar  ">
    <div class="container-fluid">
        <a class="navbar-brand" href="/about-us">Freelance for poor</a>
        <div class="main">
            <a class="button_main" href="/find-work">Find work</a>
            <a class="button_main" href="/about-us">About us</a>
        </div>
        <%if (isAuthenticated) { %>
            <div class="login" id="loginNav">
                <a class="button_main" href="/add-work">Add work</a>
                <a class="button_reg">account settings</a>
                <img src="/img/user.png" class="user_img">
                <a class="user_text" href="/logout">Logout</a>
            </div>
        <% } else{ %>
            <div class="login" id="loginNav">
                <a class="button_reg" href="/register">Get started</a>
                <a class="button_reg" href="/login">Login</a>
            </div>
        <% } %>
    </div>
</nav>
<div class="wrapper fadeInDown">
    <div id="formContent">
        <div class="fadeIn first">
            <img src="img/user.png" id="icon" alt="User Icon" />
        </div>
        <%if (isLoginError) { %>
            <div>Enter correct login and password</div>
        <% }%>
        <form id="login-form" method="POST" action="/login">
            <input type="text" id="login" class="fadeIn second" name="login" placeholder="login">
            <input type="password" id="password" class="fadeIn third" name="password" placeholder="password">

            <input type="submit" class="fadeIn fourth" value="Log In">
        </form>

    </div>
</div>
